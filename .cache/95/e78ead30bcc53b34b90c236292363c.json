","generated":{"line":119,"column":17},"source":"components/layout/SearchBar.jsx","original":{"line":7,"column":37}},{"generated":{"line":123,"column":15},"source":"components/layout/SearchBar.jsx","original":{"line":60,"column":15}},{"name":"SearchBar","generated":{"line":123,"column":40},"source":"components/layout/SearchBar.jsx","original":{"line":60,"column":24}},{"generated":{"line":123,"column":49},"source":"components/layout/SearchBar.jsx","original":{"line":60,"column":15}}],"sources":{"components/layout/SearchBar.jsx":"import React from 'react';\r\nimport classNames from 'classnames';\r\nimport { observer } from 'mobx-react';\r\nimport {API_KEY, API_BASE_URL, NUM_OF_RESULTS} from '../../constants.js';\r\nimport {debounce, fetchData} from '../../utils.js';\r\n\r\nexport class SearchBar extends React.Component {\r\n    constructor(props) {\r\n        super(props);\r\n\r\n        this.state = {\r\n            searchQuery: '',\r\n        }\r\n\r\n        this.debouncedSearch = debounce();\r\n    }\r\n\r\n    get queryParams() {\r\n        return {\r\n            searchQuery: '?query=' + this.state.searchQuery,\r\n            numberOfResults: '&number=' + NUM_OF_RESULTS,\r\n            numberOfSkippedResults: '&offset=' + 0,\r\n            apiKey: `&apiKey=${API_KEY}`,\r\n        };\r\n    }\r\n\r\n    handleSearch(e) {\r\n        e.preventDefault();\r\n\r\n        this.setState({searchQuery: e.target.value});\r\n\r\n        this.debouncedSearch(() => {\r\n            this.setState({isLoading: true});\r\n            fetchData(API_BASE_URL + 'search', this.queryParams).then(data => {\r\n                console.log('response', data);\r\n                this.setState({isLoading: false});\r\n            });\r\n        }, 280);\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"search-bar field\">\r\n                <div\r\n                    className={classNames('control is-medium', {'is-loading': this.state.isLoading})}>\r\n                    <input id=\"searchBar\"\r\n                           className=\"input is-medium\"\r\n                           type=\"text\"\r\n                           name=\"searchQuery\"\r\n                           placeholder=\"Search for a recipe...\"\r\n                           value={this.state.searchQuery}\r\n                           onChange={e => this.handleSearch(e)}\r\n                    />\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default observer(SearchBar);\r\n"},"lineCount":null}},"error":null,"hash":"af6867ec8fcb9ad099d13f02551e394c","cacheData":{"env":{}}}